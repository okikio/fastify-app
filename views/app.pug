    title= title

    include ./components/header.pug
    include ./components/layer.pug
    include ./components/layout.pug
    include ./components/title.pug
    include ./components/content.pug
    include ./components/hero.pug
    include ./components/footer.pug
  body
    +header(tabs, title)

    .header-top-spot
    main.main#swup.transition-fade
      if self
        +doc(self, false)
      if values
        each val in values
          +doc(val, false)
        each val in values
          if val.layer
            +layer(val.layer)
              if val.layer.hero
                +hero(val.layer.hero)
              if val.layer.layout
                +layout(val.layer.layout)
                  each detail in val.layer.layout.values
                    if detail.section.title
                      +title(detail.section)
                    +content(detail.section)

    +footer("2019 Okiki Ojo")
    script(src=`${dev ? './js/app.modern.min.js' : './js/app.vendor.min.js'}` inline defer)